<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="Assets/ResourceDictionaries/SolidColors.xaml" />
                <ResourceDictionary Source="Assets/ResourceDictionaries/Templates.xaml" />
                <ResourceDictionary Source="Assets/ResourceDictionaries/ImageBrushes.xaml" />
                <ResourceDictionary Source="Assets/ResourceDictionaries/Fonts.xaml" />
                <ResourceDictionary Source="Assets/ResourceDictionaries/Label.xaml" />
                <ResourceDictionary Source="Assets/ResourceDictionaries/TextBlock.xaml" />
                <ResourceDictionary Source="Assets/ResourceDictionaries/WASDKeyStyle.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>